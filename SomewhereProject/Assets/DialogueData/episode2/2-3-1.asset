  dialogues:
  - speaker: 
    line: "\uB2E4\uB4E4 \uC27D\uAC8C \uC774\uD574\uD55C \uAC83\uCC98\uB7FC \uBCF4\uC5EC\uC11C
      \uAD50\uC218\uB2D8\uAED8 \uBB3B\uAE30\uC5D4 \uBBFC\uB9DD\uD588\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uADF8\uB798\uC11C \uC870\uAD50\uB2D8\uAED8 \uBB3C\uC5B4\uBCF4\uAE30\uB85C
      \uD588\uB2E4.\n\uAC15\uC758\uC2E4\uC744 \uB098\uAC04 \uC870\uAD50\uB2D8\uC744
      \uAE09\uD558\uAC8C \uB530\uB77C\uAC14\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC800\uBC88\uC5D0 \uC5F0\uAD6C\uC2E4\uC5D0\uC11C \uBCF8 \uC774\uD6C4\uB85C
      \uC778\uC0C1\uC774 \uBCC0\uD588\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uCC98\uC74C\uC5D0\uB294 \uAC10\uC815\uC774 \uC5C6\uB294 \uC0AC\uC774\uCF54\uD328\uC2A4\uAC19\uC740
      \uB290\uB08C\uC774\uC5C8\uB294\uB370,\n\uC9C0\uAE08 \uBCF4\uB2C8\uAE4C \uC778\uAC04\uBBF8
      \uC788\uB294 \uAC83 \uAC19\uAE30\uB3C4 \uD558\uACE0..."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC870\uAD50\uB2D8, \uC9C8\uBB38\uC774 \uC788\uC2B5\uB2C8\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB4A4\uB97C \uB3C8 \uC870\uAD50\uB2D8\uC758 \uD45C\uC815\uC774 \uBB18\uD558\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "...\uC6D0\uB798 \uC870\uAD50\uD55C\uD14C\uB294 \uC9C8\uBB38 \uC798 \uC548
      \uD558\uB098?"
    eventName: 
    conditionType: 0
