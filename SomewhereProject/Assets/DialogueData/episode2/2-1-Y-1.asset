  - speaker: "\uBB38\uC774\uD604"
    line: "\uC774 \uC219\uCDE8\uD574\uC18C\uC81C \uC77C\uB2E8 \uBA39\uC5B4."
    eventName: YihyeonSaying
    conditionType: 0
