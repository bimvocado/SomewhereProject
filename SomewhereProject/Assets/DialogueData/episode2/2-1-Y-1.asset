    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC120\uBC30\uAC00 \uC219\uCDE8\uD574\uC18C\uC81C \uD558\uB098\uB97C \uAC74\uB124\uC8FC\uC168\uB2E4.\n\uBC14\uB85C
      \uBA39\uC744 \uC218 \uC788\uAC8C \uD3EC\uC7A5\uB3C4 \uB72F\uC740 \uC0C1\uD0DC\uC600\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC800\uBC88\uBD80\uD130 \uB290\uB080 \uAC70\uC9C0\uB9CC \uC815\uB9D0
      \uC12C\uC138\uD558\uACE0 \uB2E4\uC815\uD55C \uC0AC\uB78C \uAC19\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "\uC9D1 \uAC08 \uC218 \uC788\uACA0\uC5B4?"
    eventName: YihyeonSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC774\uC81C \uC9D1 \uAC00\uC57C \uD560 \uAC83 \uAC19\uC740\uB370 \uC5B4\uB5BB\uAC8C
      \uB098\uAC00\uC57C \uD560\uC9C0 \uBAA8\uB974\uACA0\uC5B4\uC694."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB0B4 \uC55E\uC5D0 \uC549\uC740 \uACFC \uB3D9\uAE30\uB4E4\uC740 \uD558\uC774\uC5D0\uB098
      \uB9C8\uB0E5 \uB0B4\uAC8C \uB610 \uC220 \uBA39\uC744 \uC0DD\uAC01\uC744 \uD558\uACE0
      \uC788\uC5C8\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uADF8 \uC21C\uAC04, \uC120\uBC30\uAC00 \uC77C\uC5B4\uB098 \uB0B4 \uAC00\uBC29\uACFC
      \uAC89\uC637\uC744 \uCC59\uACA8\uC8FC\uC168\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "\uADF8\uB7FC \uC7A0\uAE50 \uBC14\uB78C \uC880 \uC42C\uACE0 \uC628\uB2E4\uACE0
      \uD558\uACE0 \uB098\uAC00. \uB0B4\uAC00 \uB9D0\uD574\uB458\uAC8C."
    eventName: YihyeonSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB098\uB294 \uC870\uC6A9\uD788 \uC790\uB9AC\uC5D0\uC11C \uC77C\uC5B4\uB098
      \uBC14\uB78C\uC744 \uC42C\uACE0 \uC628\uB2E4\uBA70 \uBC16\uC73C\uB85C \uB098\uC654\uB2E4."
    eventName: DisappearYH
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB2E4\uD589\uD788 \uC774\uD604 \uC120\uBC30\uAC00 \uB0B4 \uC9D0\uC744
      \uCC59\uACA8\uC8FC\uC2E0 \uB355\uC5D0 \uC544\uBB34\uB3C4 \uB0B4\uAC00 \uC9D1\uC73C\uB85C
      \uD284\uB2E4\uACE0 \uC758\uC2EC\uD558\uC9C0 \uC54A\uC558\uB2E4."
    eventName: GoingHome
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC120\uBC30\uB3C4 \uC548\uC5D0\uC11C \uB2E4\uB978 \uC120\uBC30\uB2D8\uAED8
      \uBB54\uAC00 \uB9D0\uC744 \uD558\uC2DC\uB354\uB2C8 \uB530\uB77C \uB098\uC624\uC168\uB2E4."
