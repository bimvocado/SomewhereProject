    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uD558\uB098\uB458\uC529 \uC0AC\uB78C\uB4E4\uC774 \uB3C4\uCC29\uD558\uACE0
