    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uD558\uD558...\n\uBB54\uAC00 \uC774\uCABD\uC73C\uB85C \uD5A5\uD558\uB294
