    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC55E\uC790\uB9AC \uC120\uBC30"
    line: "\uD559\uAD50 \uADFC\uCC98 \uD0D0\uBC29\uC740 \uC880 \uD574\uBD24\uC5B4?
