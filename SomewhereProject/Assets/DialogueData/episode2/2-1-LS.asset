    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC55E\uC790\uB9AC \uC120\uBC30\uB2D8\uC740 \uC544\uB294\uAC8C \uAD49\uC7A5\uD788
