    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "\uC751. \uC9D1 \uADFC\uCC98\uACE0, \uC870\uC6A9\uD574\uC11C."
    eventName: YihyeonSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "\uAD50\uC7AC\uB3C4 \uC5EC\uAE30\uC11C \uB9CE\uC774 \uC0C0\uC5C8\uACE0."
    eventName: YihyeonSaying
    conditionType: 0
