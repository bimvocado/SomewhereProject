    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC9D1\uC5D0 \uC788\uC73C\uBA74 \uC2DC\uAC04\uB9CC \uBD95 \uB5A0\uC11C
