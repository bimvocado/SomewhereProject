    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC120\uBC30? \uC5EC\uAE30\uC11C \uBB50 \uD558\uC138\uC694?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "\uADF8\uB0E5\xB7\xB7\xB7. \uCC45 \uC880 \uBCF4\uB7EC \uC654\uC5B4."
