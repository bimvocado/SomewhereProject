    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC640, \uC2E0\uAE30\uD558\uB2E4. \uC800\uB3C4\uC694!"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "\uC5EC\uAE30 \uC790\uC8FC \uC640?"
    eventName: YihyeonSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uAC00\uB054\uC694. \uADF8\uB0E5 \uCC45 \uAD6C\uACBD\uD558\uB294 \uAC70
