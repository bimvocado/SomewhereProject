    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB178\uD2B8 \uD558\uB098 \uC0AC\uACE0, \uCC45 \uC880 \uAD6C\uACBD\uD558\uACE0."
    eventName: YihyeonCenter
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC0AC\uC2E4\uC740 '\uB098 \uACF5\uBD80\uD558\uACE0 \uC788\uB2E4!' \uAC19\uC740
      \uAE30\uBD84\uC744 \uB0B4\uACE0 \uC2F6\uC5C8\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\xB7\xB7\xB7"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC11C\uC810\uC740 \uC758\uC678\uB85C \uC0AC\uB78C\uC774 \uB9CE\uC558\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC785\uAD6C \uCABD \uBCA0\uC2A4\uD2B8\uC140\uB7EC \uCF54\uB108\uBD80\uD130
      \uCC9C\uCC9C\uD788 \uC548\uC73C\uB85C \uAC78\uC5B4 \uB4E4\uC5B4\uAC14\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB3C5\uB9BD \uCD9C\uD310 \uC139\uC158 \uC55E\uC5D0\uC11C \uBA48\uCDC4\uC744
      \uB54C,"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC67C\uCABD \uC5B4\uAE68 \uB108\uBA38\uB85C \uC775\uC219\uD55C \uAE30\uCC99\uC774
      \uB290\uAEF4\uC84C\uB2E4."
    eventName: AppearYH
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
