    eventName: GotoVHallway
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "90x\uD638 \uC55E\uC5D0\uC11C \uBB38\uB4DD \uBA70\uCE60 \uC804 \uC77C\uC774
