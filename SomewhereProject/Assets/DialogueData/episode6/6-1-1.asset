    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uBB54\uAC00 \uCC98\uC74C\uC73C\uB85C \uB3CC\uC544\uAC04 \uB290\uB08C\uC774\uB2E4.
      \uCC98\uC74C \uBD24\uC744 \uB54C \uADF8 \uBB34\uB69D\uB69D\uD55C \uD45C\uC815\uC73C\uB85C
