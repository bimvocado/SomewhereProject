    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC544 \uBAB0\uB77C\xB7\xB7\xB7. \uBE68\uB9AC \uBC84\uB9AC\uB7EC \uAC00\uC57C\uC9C0.
