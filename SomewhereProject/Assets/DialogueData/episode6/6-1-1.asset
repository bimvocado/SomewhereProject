    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC77C\uBC18 \uC4F0\uB808\uAE30\uBD09\uD22C \uB2E4 \uB5A8\uC5B4\uC84C\uB098?
      \uBD84\uB9AC\uC218\uAC70 \uD558\uB294 \uAE40\uC5D0 \uC0AC \uC640\uC57C\uACA0\uB2E4."
