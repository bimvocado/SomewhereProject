    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC774\uC820 \uC800 \uC0AC\uB78C\uC774 \uC880 \uC9DC\uC99D \uB09C\uB2E4."
