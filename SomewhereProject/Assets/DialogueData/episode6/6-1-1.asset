    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC6B0\uC9C4 \uC120\uBC30\uAC00 \uB098\uB791 \uC544\uBB34 \uC0AC\uC774\uB85C\uB3C4
      \uC0DD\uAC01 \uC548 \uD558\uB294 \uAC83 \uAC19\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uADF8\uB7FC, \uBB54\uB370?"
    eventName: 
    conditionType: 0
