  dialogues:
  - speaker: 
    line: "\uC774\uB7F0\uC800\uB7F0 \uC598\uAE30\uB97C \uD558\uB2E4 \uBCF4\uB2C8
      \uC11C\uC810 \uAE4A\uC219\uD55C \uCABD\uC744 \uD568\uAED8 \uAC77\uAC8C \uB418\uC5C8\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB9D0\uC740 \uB9CE\uC774 \uC5C6\uC5C8\uC9C0\uB9CC, \uAC00\uB054 \uB098\uB791
      \uB208\uC774 \uB9C8\uC8FC\uCE60 \uB54C\uB9C8\uB2E4 \uC120\uBC30\uB294 \uC544\uC8FC
      \uC791\uAC8C \uC6C3\uC5C8\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB0B4\uAC00 \uBB38\uD559 \uCF54\uB108 \uC55E\uC5D0\uC11C \uBA48\uCD94\uC790,
      \uC120\uBC30\uAC00 \uBB3C\uC5C8\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "\uC18C\uC124 \uC88B\uC544\uD574?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uB124. \uC18C\uC124\uC774\uBA74 \uC774\uAC83\uC800\uAC83 \uB2E4 \uBCF4\uB294
      \uAC83 \uAC19\uC544\uC694."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "\uADF8\uB807\uAD6C\uB098. \uD55C \uAD8C \uCD94\uCC9C\uD574 \uC904\uB798?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\xB7\xB7\xB7 \uB124?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "[\uC5EC\uC8FC\uC774] \uB124\uAC00 \uCD94\uCC9C\uD574 \uC8FC\uB294 \uCC45
      \uC77D\uC5B4\uBCF4\uACE0 \uC2F6\uC5B4\uC11C."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "\uC77D\uB294 \uCC45\uC744 \uBCF4\uBA74 \uADF8 \uC0AC\uB78C\uC744 \uC54C
      \uC218 \uC788\uB2E4\uACE0 \uD558\uC796\uC544."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC120\uBC30\uB294 \uB0B4\uAC00 \uC5B4\uB5A4 \uC0AC\uB78C\uC778\uC9C0
      \uAD81\uAE08\uD55C \uAC74\uAC00. \uC870\uAE08 \uBD80\uB044\uB7FD\uAE30\uB3C4
      \uD55C\uB370\xB7\xB7\xB7. \uBB50\uB77C\uACE0 \uB9D0\uD558\uC9C0."
