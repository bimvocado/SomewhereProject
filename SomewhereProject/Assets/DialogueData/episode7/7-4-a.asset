    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uBC14\uB78C \uC880 \uC42C\uACE0 \uC2F6\uC740\uB370, \uC2DC\uAC04 \uB3FC?"
    eventName: SehaCenter
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uAC11\uC790\uAE30?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uC9C0\uAE08 \uC544\uB2C8\uBA74 \uBABB \uBD88\uB7EC\uB0BC \uAC83 \uAC19\uC544\uC11C."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uADF8\uB807\uAC8C \uB3C4\uCC29\uD55C \uC791\uC740 \uCE74\uD398."
    eventName: GotoCafeSH
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uD587\uC0B4\uC774 \uB208 \uBD80\uC2DC\uAE34 \uD558\uC9C0\uB9CC, \uCE74\uD398\uD14C\uB77C\uC2A4
      \uC790\uB9AC\uB294 \uADF8\uB298 \uC544\uB798\uB77C \uC81C\uBC95 \uC2DC\uC6D0\uD588\uB2E4."
    eventName: AppearSeha
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC6D0\uB798 \uC774\uB807\uAC8C \uAC11\uC790\uAE30 \uC0AC\uB78C \uBD80\uB974\uACE0
      \uADF8\uB798?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uADF8\uB0E5, \uC9C0\uAE08 \uC544\uB2C8\uBA74 \uC548 \uB420 \uAC83 \uAC19\uC544\uC11C.
      \uAC11\uC790\uAE30 \uBD10\uC57C \uD560 \uAC83 \uAC19\uB2E4\uB294 \uC0DD\uAC01\uC774
      \uB4E4\uC5C8\uC5B4."
    eventName: SehaSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB9C8\uC8FC \uC549\uC544 \uC2DC\uC6D0\uD55C \uC74C\uB8CC\uB97C \uB9C8\uC2DC\uBA70,
      \uC544\uBB34 \uB9D0 \uC5C6\uC774 \uBA87 \uBD84 \uC815\uB3C4\uB97C \uADF8\uB0E5
      \uADF8\uB807\uAC8C \uBCF4\uB0C8\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uB108 \uCD95\uC81C \uB54C, \uB0B4 \uBB34\uB300 \uBD24\uC5C8\uC5B4?"
    eventName: SehaSaying
    conditionType: 2
    conditionKey: stageSH
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC751, \uBD24\uC5B4."
    eventName: 
    conditionType: 2
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uB108 \uCD95\uC81C \uB54C, \uB0B4 \uBB34\uB300 \uBD24\uB2E4\uACE0 \uD588\uC5C8\uC9C0."
    eventName: SehaSaying
    conditionType: 1
    conditionKey: stageSH
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC751, \uB9DE\uC544."
    eventName: 
    conditionType: 1
    conditionKey: stageSH
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uADF8 \uB4A4\uB85C\xB7\xB7\xB7 \uB098\uC5D0 \uB300\uD574 \uBB50 \uC880
      \uB4E4\uC5C8\uC5B4?"
    eventName: SehaSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uADF8 \uB9D0\uC5D0 \uC790\uC5F0\uC2A4\uB7FD\uAC8C \uBA70\uCE60 \uC804
      \uCC3E\uC544\uBD24\uB358 \uAE00\uB4E4\uC774 \uB5A0\uC62C\uB790\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB178\uB798 \uBD80\uB978 \uC560, \uC608\uC804\uC5D0 \uC544\uC774\uB3CC
      \uC5F0\uC2B5\uC0DD\uC774\uC5C8\uB2E4\uB354\uB77C.'"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uCDA4 \uC601\uC0C1 \uC788\uB358\uB370 \uC9C4\uC9DC \uC798 \uCD94\uB358\uB370.'"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC5F0\uC608\uC778 \uB420 \uBED4\uD588\uB2E4\uB358\uB370 \uC65C \uB300\uD559
      \uC654\uB300?'"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC751, \uBB50\xB7\xB7\xB7 \uC870\uAE08\uC740."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uC0AC\uC2E4 \uADF8 \uC598\uAE30, \uB098\uC911\uC5D0 \uD558\uB824\uACE0
      \uD588\uB294\uB370\xB7\xB7\xB7"
    eventName: SehaSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uC9C0\uAE08\uCBE4\uC774\uBA74 \uB9D0\uD574\uB3C4 \uB420 \uAC83 \uAC19\uB354\uB77C.
      \uB108\uD55C\uD14C."
    eventName: SehaSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC65C \uB098\uC911\uC5D0 \uB9D0\uD558\uB824\uACE0 \uD588\uB294\uB370?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uAD1C\uD788 \uC774\uC0C1\uD558\uAC8C \uBCF4\uC77C\uAE4C \uBD10."
    eventName: SehaSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uC5F0\uC2B5\uC0DD\uC774\uC5C8\uB358 \uAC70 \uB9D0\uD558\uBA74, \uADF8\uB0E5
      \uAC89\uB3C4\uB294 \uC0AC\uB78C\uCC98\uB7FC \uB290\uAEF4\uC9C8 \uC218\uB3C4
      \uC788\uACE0\xB7\xB7\xB7"
    eventName: SehaSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uADFC\uB370 \uC65C \uACB0\uAD6D \uB9D0\uD558\uAE30\uB85C \uD55C \uAC70\uC57C?"
    eventName: SehaSerious
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\xB7\xB7\xB7\uB124\uAC00 \uB098 \uBD88\uD3B8\uD574\uD558\uB294 \uAC83
      \uAC19\uC544\uC11C."
    eventName: SehaSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uB098\uB294 \uB108\uB791 \uBD88\uD3B8\uD574\uC9C0\uAE30 \uC2EB\uAC70\uB4E0\xB7\xB7\xB7."
    eventName: SehaUpset
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uAC15\uC138\uD558"
    line: "\uADFC\uB370, \uADF8\uB7EC\uB824\uBA74\xB7\xB7\xB7 \uC774 \uC598\uAE30\uB97C
      \uAEBC\uB0B4\uC57C \uD55C\uB2E4\uACE0 \uC0DD\uAC01\uD588\uC5B4."
    eventName: SehaSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
