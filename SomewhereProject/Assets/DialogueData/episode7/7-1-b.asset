    eventName: WoojinSaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC640, \uC778\uACF5\uC9C0\uB2A5\uC774\uB124!"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
