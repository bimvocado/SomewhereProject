  dialogues:
  - speaker: 
    line: "\uACE0\uB978 \uC637\uC744 \uC785\uACE0 \uC774\uB7F0\uC800\uB7F0 \uC900\uBE44\uB97C
      \uD558\uB2E4 \uBCF4\uB2C8 \uC2DC\uAC04\uC774 \uAE08\uBC29 \uD758\uB800\uB2E4."
    eventName: 
    conditionType: 0
