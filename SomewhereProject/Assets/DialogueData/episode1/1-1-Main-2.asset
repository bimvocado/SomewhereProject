  m_Name: 1-1-Main-2
  m_EditorClassIdentifier: 
  dialogues:
  - speaker: 
    line: "\uC774\uC81C \uACA8\uC6B0 \uC778\uC0AC\uB97C \uB098\uB204\uC5C8\uB294\uB370,
      \uB610 \uC790\uB9AC\uB97C \uBC14\uAFB8\uB77C\uB294 \uD68C\uC7A5 \uC120\uBC30\uC758
      \uBAA9\uC18C\uB9AC\uAC00 \uB4E4\uB824\uC654\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC870\uAE08 \uC544\uC27D\uC9C0\uB9CC \uC790\uB9AC\uB97C \uC62E\uACBC\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uADF8 \uB4A4\uB85C\uB3C4 \uBA87 \uBC88\uC774\uB098 \uBC14\uB00C\uB294
      \uC606\uC0AC\uB78C\uACFC \uC778\uC0AC\uB97C \uB098\uB204\uC5C8\uACE0\n\uC194\uC9C1\uD788
      \uC774\uC820 \uB204\uAC00 \uB204\uAD6C\uC600\uB294\uC9C0 \uAE30\uC5B5\uB3C4
      \uC798 \uC548 \uB09C\uB2E4..."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB2E4\uD589\uD788\uB3C4 \uC7A0\uC2DC \uC26C\uB294 \uC2DC\uAC04\uC774
      \uC8FC\uC5B4\uC84C\uB2E4.\n\uD654\uC7A5\uC2E4\uC774\uB77C\uB3C4 \uB2E4\uB140\uC640\uC57C\uC9C0."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uD654\uC7A5\uC2E4\uC5D0 \uB4E4\uB800\uB2E4 \uB2E4\uC2DC \uAC15\uC758\uC2E4\uB85C
      \uB3CC\uC544\uC654\uB294\uB370, \uC870\uAE08 \uC804\uAE4C\uC9C0 \uB0B4\uAC00
      \uC549\uC544 \uC788\uB358 \uC790\uB9AC\uB97C \uB2E4\uB978 \uC0AC\uB78C\uC774
      \uCC28\uC9C0\uD558\uACE0 \uC788\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uBE44\uCF1C\uB2EC\uB77C\uACE0 \uD558\uAE30\uC5D0\uB3C4 \uC560\uB9E4\uD558\uACE0...\n\uADF8\uB0E5
      \uB0B4\uAC00 \uB2E4\uB978 \uC790\uB9AC\uB97C \uCC3E\uC544\uC57C\uACA0\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB0A8\uC740 \uC790\uB9AC\uAC00,"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uCC98\uC74C\uC5D0 \uBCF8 \uC131\uACA9 \uC548 \uC88B\uC744 \uAC83\uAC19\uC774
      \uC0DD\uAE34 \uB0A8\uC790\uC758 \uC606..?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC800 \uC0AC\uB78C \uC870\uAE08 \uC131\uACA9 \uC548 \uC88B\uC544 \uBCF4\uC774\uB294\uB370,
      \uC606\uC5D0 \uC549\uC544\uB3C4 \uB418\uB098?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC8FC\uBCC0 \uC790\uB9AC \uB2E4 \uBE44\uC5B4 \uC788\uB294\uB370 \uD639\uC2DC
      \uD63C\uC790 \uC788\uACE0 \uC2F6\uC5B4\uD558\uB294\uAC74\uAC00..?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC77C\uB2E8 \uBB3C\uC5B4\uBCF4\uAE30\uB77C\uB3C4 \uD558\uC790."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC548\uB155\uD558\uC138\uC694! \uC606\uC5D0 \uC549\uC544\uB3C4 \uB420\uAE4C\uC694?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: ???
    line: "\uC5B4?"
    eventName: smile
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB0A8\uC790\uAC00 \uACE0\uAC1C\uB97C \uB4E4\uC5B4 \uB098\uB97C \uCCD0\uB2E4\uBD24\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uAD1C\uD788 \uBB3C\uC5B4\uBD24\uB098? \uBB54\uAC00 \uAE30\uBD84\uC774
      \uC548 \uC88B\uC544 \uBCF4\uC774\uB124..."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC544, \uC790\uB9AC \uC788\uB098\uC694? \uADF8\uB7FC \uB2E4\uB978\uB370,"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: ???
    line: "\uC544\uB2C8, \uC544\uB1E8! \uC790\uB9AC \uC5C6\uC5B4\uC694! \uC549\uC73C\uC154\uB3C4
      \uB3FC\uC694!"
    eventName: smile
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB0A8\uC790\uAC00 \uC870\uC2EC\uC2A4\uB7FD\uAC8C \uB0B4 \uC190\uBAA9\uC744
      \uC7A1\uC558\uB2E4\uAC00 \uB193\uC558\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: ???
    line: "\uC544 \uD639\uC2DC \uC549\uAE30 \uC2EB\uC73C\uC2DC\uBA74 \uB2E4\uB978
      \uACF3 \uC549\uC73C\uC154\uB3C4 \uB3FC\uC694. \uAD1C\uCC2E\uC544\uC694."
    eventName: smile
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC5EC\uAE30 \uC549\uC744\uAC8C\uC694."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC5EC\uC804\uD788 \uAD1C\uCC2E\uC740 \uAC74\uC9C0 \uBAA8\uB974\uACA0\uB294
      \uC5BC\uAD74\uC774\uB2E4.\n\uB0B4\uAC00 \uAE30\uBD84 \uC548 \uC88B\uAC8C \uD55C
      \uAC74 \uC544\uB2C8\uACA0\uC9C0?"
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC790\uB9AC\uC5D0 \uC549\uC73C\uBA70 \uB208\uCE58\uB97C \uC0B4\uD53C\uB294\uB370,
      \uB0A8\uC790\uAC00 \uB9D0\uC744 \uAC78\uC5B4\uC654\uB2E4."
    eventName: smile
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC774\uC740\uC131"
    line: "\uD639\uC2DC \uC774\uB984\uC774 \uC5B4\uB5BB\uAC8C \uB3FC\uC694?\n\uC544,
      \uC800\uB294 \uC774\uC740\uC131\uC774\uC5D0\uC694."
    eventName: smile
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "[\uC5EC\uC8FC\uC608\uC694]."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC774\uC740\uC131"
    line: "[\uC5EC\uC8FC] \uC528. \uB9CC\uB098\uC11C \uBC18\uAC00\uC6CC\uC694."
    eventName: smile
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC800\uB3C4 \uB9CC\uB098\uC11C \uBC18\uAC00\uC6CC\uC694, \uC740\uC131
      \uC528."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC774\uC740\uC131"
    line: "\uC544! \uC800\uB294, \uC2A4\uBB3C... \uC74C, \uC544\uB2C8\uB2E4.\n\uC800
      \uBA87 \uC0B4 \uAC19\uC544 \uBCF4\uC5EC\uC694?"
    eventName: smile
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
