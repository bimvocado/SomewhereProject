    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uAC15\uC758 \uC790\uB8CC\uC5D0 \uAD34\uBC1C\uAC1C\uBC1C \uC801\uD78C
      \uAE00\uC528\uAC00 \uC9C0\uB801\uC774\uB9C8\uB0E5 \uAE30\uC5B4\uAC00\uACE0
