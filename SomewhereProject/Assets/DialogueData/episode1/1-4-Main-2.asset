    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB290\uB9BF\uD558\uAC8C \uAC00\uBC29\uC744 \uC815\uB9AC\uD558\uB358 \uC911,
      \uC606\uC5D0\uC11C \uC2DC\uC120\uC774 \uB290\uAEF4\uC84C\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
