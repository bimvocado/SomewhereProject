    line: "\uACE0\uAC1C\uB97C \uB3CC\uB9AC\uB2C8 \uC740\uC131\uC774 \uC11C \uC788\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
