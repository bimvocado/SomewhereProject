    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC7A1\uC9C0 \uB9D0 \uAC78..."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC218\uC5C5\uC2DC\uAC04 \uB0B4\uB0B4 \uC878\uC740 \uAC83 \uAC19\uB2E4.
