    eventName: smile
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC740\uC131\uC774 \uD5E4\uC2E4\uD5E4\uC2E4 \uC6C3\uC73C\uBA70 \uC190\uC744
