    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB9CC\uB9CC\uD560 \uC904 \uC54C\uC558\uB294\uB370 \uC27D\uC9C0\uAC00
