    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uADF8\uB7F0\uB370 \uC5B4\uCA4C\uB2E4\uBCF4\uB2C8 \uACC4\uC18D \uAC19\uC740
