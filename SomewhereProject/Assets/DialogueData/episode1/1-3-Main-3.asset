    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC774\uAC8C \uBB54\uAC00...\n\uBB18\uD558\uAC8C \uC5B4\uC0C9\uD588\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC120\uBC30\uB2D8\uB3C4 \uC0B4\uC9DD \uC6C3\uC73C\uBA70 \uB9D0\uD588\uB2E4."
    eventName: 
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "\uD639\uC2DC... \uD559\uC2DD \uB4DC\uC2DC\uB7EC \uAC00\uC138\uC694?"
