    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uBB38\uC774\uD604"
    line: "\uC557 \uB124. \uC800\uB3C4 \uD559\uC2DD\uB2F9 \uAC11\uB2C8\uB2E4."
    eventName: YHsaying
    conditionType: 0
    conditionKey: 
    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uADF8\uB807\uAC8C \uB3C4\uCC29\uD55C \uD559\uC2DD\uB2F9\uC740 \uC6B4
      \uB098\uC058\uAC8C\uB3C4 \uC790\uB9AC\uAC00 \uB2E4 \uCC28 \uC788\uC5C8\uB2E4..."
    eventName: GotoCafeteria
