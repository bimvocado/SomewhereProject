    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: "\uC5EC\uC8FC"
    line: "\uC800\uBC88 \uD559\uAE30\uC5D4 \uBABB \uC7A1\uC740 \uAC15\uC88C \uC788\uC5C8\uB2E8
