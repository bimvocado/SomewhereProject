    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uB108\uBB34 \uD765\uBD84\uD574\uC11C \uC18C\uB9AC\uAE4C\uC9C0 \uC9C0\uB97C
