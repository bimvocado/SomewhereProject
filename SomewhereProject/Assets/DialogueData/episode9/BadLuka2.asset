    conditionTargetCharacter: 
    conditionValue: 0
    hasChoices: 0
    choices: []
  - speaker: 
    line: "\uC218\uAC15 \uC2E0\uCCAD\uC744 \uC62C\uD074\uD558\uACE0 \uAC00\uBCBC\uC6B4
